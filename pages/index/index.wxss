/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.circle {
  position: absolute;
  bottom: 61.8%;
  left: 50%;
  transform: translate(-50%, 61.8%);
  display: flex;
  justify-content: center;
  align-items: center;
  width: 400rpx;
  height: 400rpx;
  border-radius: 50%;
  border: 1px solid #cccccc;
  font-size: 100rpx;
  color: #999999;
}